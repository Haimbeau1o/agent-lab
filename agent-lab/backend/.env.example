NODE_ENV=development
PORT=3001

# Database
DATABASE_URL="file:./dev.db"

# Default API Configuration (for testing)
DEFAULT_API_KEY=your_openai_api_key_here
DEFAULT_BASE_URL=https://api.openai.com/v1
DEFAULT_MODEL=gpt-4

# Security - REQUIRED for production
# Generate a random 32+ character encryption key
ENCRYPTION_KEY=your_32_character_encryption_key_here_change_this
# Generate a random 32 hex character (16 bytes) salt: node -e "console.log(require('crypto').randomBytes(16).toString('hex'))"
ENCRYPTION_SALT=your_32_hex_character_salt_here_change_this

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001
